{
  "extends": "tslint:latest",
  "defaultSeverity": "error",
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "no-implicit-dependencies": [true, "dev"],
    "no-console": [true, "log"],
    "arrow-parens": false,
    "no-unused-expression": false,
    "ordered-imports": [
      true, 
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-last"
      }
    ]
  }
}
