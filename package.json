{
  "name": "rsschool-api",
  "version": "0.0.1",
  "description": "API for accessing rsschool",
  "repository": "github:rolling-scopes/rsschool-api",
  "scripts": {
    "build": "tsc",
    "start": "nodemon --watch 'app/**/*' -e ts --exec 'ts-node' ./index.ts | bunyan -o short",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "pretty": "prettier --config .prettierrc.json --write \"./app/**/*.ts\"",
    "test": "mocha --require ts-node/register \"./app/**/*.spec.ts\"",
    "coverage": "node_modules/nyc/bin/nyc.js --reporter=html --reporter=text node_modules/mocha/bin/mocha --exit -r ts-node/register ./app/**/*.spec.ts",
    "coverage:coveralls": "node_modules/nyc/bin/nyc.js --reporter=lcov --reporter=text-lcov node_modules/mocha/bin/mocha --exit -r ts-node/register ./app/**/*.spec.ts | coveralls"
  },
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "2.2.1",
    "@octokit/rest": "15.8.1",
    "@types/csv-parse": "1.1.11",
    "bunyan": "2.0.2",
    "csv-parse": "2.4.0",
    "http-status-codes": "1.3.0",
    "koa": "2.5.1",
    "koa-bodyparser": "4.2.1",
    "koa-passport": "4.1.0",
    "koa-router": "7.4.0",
    "koa-session": "5.8.1",
    "koa-static": "4.0.3",
    "koa2-ratelimit": "0.5.0",
    "koa2-swagger-ui": "2.9.1",
    "mongoose": "5.1.4",
    "passport-github": "1.1.0"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.4",
    "@types/chai-http": "3.0.4",
    "@types/chai": "4.1.3",
    "@types/koa__cors": "2.2.2",
    "@types/koa-bodyparser": "4.2.0",
    "@types/koa-passport": "4.0.1",
    "@types/koa-router": "7.0.28",
    "@types/koa-session": "5.7.4",
    "@types/koa-static": "4.0.0",
    "@types/koa": "2.0.46",
    "@types/lodash": "4.14.109",
    "@types/mocha": "5.2.1",
    "@types/mongoose": "5.0.15",
    "@types/node": "10.3.1",
    "@types/passport-github": "1.1.3",
    "chai-http": "3.0.0",
    "chai": "4.1.2",
    "coveralls": "3.0.1",
    "koa-async-validator": "0.1.2",
    "mocha-lcov-reporter": "1.3.0",
    "mocha": "5.2.0",
    "nodemon": "1.17.5",
    "nyc": "12.0.2",
    "prettier": "1.13.4",
    "source-map-support": "0.5.6",
    "ts-node": "6.1.0",
    "tslint": "5.10.0",
    "typescript": "2.9.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "excludes": [
      "**/*.d.ts",
      "./test/**/*"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
